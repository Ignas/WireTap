<wiretap_layout>
  <size>1024x768</size>
  <background src="graphics/Background.png" />
  <console>
    <speaking_indicator x="215" y="70">
      <inactive src="graphics/Lamp_inactive.png" />
      <active src="graphics/Lamp_active.png" />
      <blink src="graphics/Lamp_blink.png" /> <!-- currently unused -->
      <done src="graphics/Lamp_done.png" /> <!-- currently unused -->
    </speaking_indicator>
    <listening_button x="104" y="40" width="128" height="64">
      <off src="graphics/Off.png" />
      <on src="graphics/On.png" />
    </listening_button>
    <swat_button x="104" y="100" width="128" height="64">
      <off src="graphics/Send_swat.png" />
      <on src="graphics/Send_swat_pressed.png" />
      <disabled src="graphics/Send_swat_inactive.png" /> <!-- currently unused -->
    </swat_button>
  </console>
  <grid x="0" y="0" rows="4" cols="4" width="256" height="150" />
  <coffee_break_button x="132" y="651" width="128" height="64">
    <off src="graphics/Break.png" />
    <on src="graphics/Break_pressed.png" />
  </coffee_break_button>
  <quit_button x="888" y="651" width="128" height="64">
    <off src="graphics/Quit.png" />
    <on src="graphics/Quit_pressed.png" />
  </quit_button>
</wiretap_layout>
